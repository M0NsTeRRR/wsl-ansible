apiVersion: v1
kind: Config
preferences: {}
current-context: homelab-dev
clusters:
  - name: homelab
    cluster:
      server: https://k8s.unicornafk.fr
users:
  - name: lortega
    user:
      client-certificate: /home/{{ username_on_the_host }}/.kube/certs/lortega.pem
      client-key: /home/{{ username_on_the_host }}/.kube/certs/lortega.key
contexts:
  - name: homelab-dev
    context:
      cluster: homelab
      namespace: dev
      user: lortega
